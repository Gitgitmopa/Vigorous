<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vigorous">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Vigorous">
        <activity android:name=".MaticSplashActivity"></activity>
        <activity android:name=".Control" />
        <activity android:name=".InstructionActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".SignAndRegActivity" />
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".SplashTwoActivity" />
        <activity android:name=".SplashActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoadingActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>